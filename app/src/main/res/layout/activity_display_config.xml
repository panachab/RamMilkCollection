<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:paddingTop="@dimen/activity_vertical_margin">

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/scrollview_margintop"
        android:layout_below="@+id/header">

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:contextClickable="true"
            android:focusable="true"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/ClientidTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">
                    <EditText
                        android:id="@+id/editClientId"
                        android:inputType="number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:hint="*Client Id"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/VentorWerksTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">
                    <EditText
                        android:id="@+id/editVentorWerks"
                        android:inputType="number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:hint="*Κωδικός Εργοστασίου"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/UrlTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">
                    <EditText
                        android:id="@+id/editWebServiceUrl"
                        android:inputType="text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:hint="*WebService Url"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/dsr0codeTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editDsr0Code"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:hint="*Σειρά Παραστατικών ΦΠΑ=0"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/dsr1codeTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editDsr1Code"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:hint="*Σειρά Παραστατικών ΦΠΑ=1"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/checkdsrcodeTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editCheckDsrCode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:hint="*Σειρά Παραστατικών Ελέγχου Παραλαβών"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/diakinisidsrcodeTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editDiakinisiDsrCode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:hint="*Σειρά Παραστατικών Ενδοδιακινήσεων"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/metaggisidsrcodeTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editMetaggisiDsrCode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:hint="*Σειρά Παραστατικών Μεταγγίσεων"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/checkdMetaggisiDrcodeTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editCheckMetaggisiDsrCode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:hint="*Σειρά Παραστατικών Ελέγχου Μεταγγίσεων"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/dsr0numTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editDsr0Num"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="14sp"
                        android:hint="*Αριθμός Σειράς  ΦΠΑ=0"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/dsr1numTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editDsr1Num"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="14sp"
                        android:hint="*Αριθμός Σειράς ΦΠΑ=1"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/checkdsrnumTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editCheckDsrNum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="14sp"
                        android:hint="*Αριθμός Σειράς Ελέγχου Παραλαβών"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/diakinisidsrnumTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editDiakinisiDsrNum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="14sp"
                        android:hint="*Αριθμός Σειράς Ενδοδιακινήσεων"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/metaggisidsrnumTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editMetaggisiDsrNum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="14sp"
                        android:hint="*Αριθμός Σειράς Μεταγγίσεων"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/checkmetaggisidsrnumTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editCheckMetaggisiDsrNum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textSize="14sp"
                        android:hint="*Αριθμός Σειράς Ελέγχου Μεταγγίσεων"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/printerTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_span="2">

                    <EditText
                        android:id="@+id/editPrinter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:textSize="14sp"
                        android:hint="*Ονομασία Εκτυπωτή"/>

                </android.support.design.widget.TextInputLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/button1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_span="2"
                        android:background="@drawable/image_button"
                        android:drawableLeft="@drawable/ic_system_update_alt_white_18dp"
                        android:onClick="run"
                        android:padding="10dp"
                        android:text="@string/save"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#FFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/button_checkid"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_span="2"
                        android:background="@drawable/image_button"
                        android:drawableLeft="@drawable/ic_expand_00015"
                        android:onClick="checkid"
                        android:padding="10dp"
                        android:text="Αποστολή ID"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#FFF"
                        android:textSize="14sp" />
                </LinearLayout>

            </TableRow>

        </TableLayout>
    </ScrollView>

</RelativeLayout>



